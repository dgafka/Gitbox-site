<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Gitbox\ApplicationBundle\Entity\UserDescription" table="user_description">
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="description" type="text" column="description" nullable="true"/>
    <field name="registrationDate" type="date" column="registration_date" nullable="false"/>
    <field name="banDate" type="date" column="ban_date" nullable="true"/>
    <field name="ip" type="string" column="ip" length="15" nullable="true"/>
    <field name="hit" type="integer" column="hit" nullable="false"/>
  </entity>
</doctrine-mapping>
